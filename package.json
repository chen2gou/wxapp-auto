{
	"name": "wxapp-auto",
	"displayName": "wxapp-auto",
	"description": "WeApp Snippets",
	"version": "0.0.5",
	"publisher": "jhs1873",
	"keywords": [
		"小程序",
		"wx",
		"wxapp",
		"微信小程序",
		"wepy-wx"
	],
	"icon": "wxapp.png",
	"engines": {
		"vscode": "^1.18.0"
	},
	"categories": [
		"Snippets"
	],
	"contributes": {
		"languages": [
			{
				"id": "vue",
				"aliases": [
					"Vue",
					"vue"
				],
				"extensions": [
					".vue"
				],
				"configuration": "./syntaxes/vue.config.json"
			}
		],
		"grammars": [
			{
				"language": "vue",
				"scopeName": "text.html.vue",
				"path": "./syntaxes/vue.template"
			}
		],
		"snippets": [
			{
				"language": "html",
				"path": "./snippets/wxml.json"
			},
			{
				"language": "javascript",
				"path": "./snippets/javascript.json"
			},
			{
				"language": "json",
				"path": "./snippets/json.json"
			},
			{
				"language": "css",
				"path": "./snippets/css.json"
			},
			{
				"language": "vue",
				"path": "./snippets/wxml.json"
			}
		]
	},
